<Page x:Class="MenshakovaBytService.Pages.CustomerPages.CustomerMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MenshakovaBytService.Pages.CustomerPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Профиль пользователя">

    <Grid>
        <StackPanel>
            <Button HorizontalAlignment="Right"
                    Width="100"
                    Height="25"
                    Content="Выйти"
                    Margin="5"
                    Name="BtnExit"
                    Click="BtnExit_Click"/>
            <TextBlock Name="TxbUserName"
                       Text="Name"
                       FontSize="20"
                       Margin="5"/>
            <TextBlock Name="TxbUserType"
                       Text="Name"
                       FontSize="18"
                       Margin="5"/>
            <TextBlock Name="TxbPhoneNumber"
                       Text="Name"
                       FontSize="18"
                       Margin="5"/>
        </StackPanel>
    </Grid>
</Page>
